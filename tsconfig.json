{
    "compilerOptions": {
      "target": "ES6",                          
      "module": "commonjs",                     
      "rootDir": "./src",                       
      "outDir": "./dist",                       
      "strict": true,                           
      "esModuleInterop": true,                  
      "skipLibCheck": true,                     
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "sourceMap": true
    },
    "include": [
      "src/**/*.ts"
, "src/server.ts"    ],
    "exclude": [
      "node_modules"
    ]
  }
  